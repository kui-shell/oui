{
  "name": "oui",
  "version": "0.0.0",
  "description": "",
  "main": "node_modules/@kui-shell/core/main/main.js",
  "scripts": {
    "build:electron:mac": "PLATFORM=mac kui-build-electron",
    "build:electron:linux": "PLATFORM=linux kui-build-electron",
    "build:electron:win32": "PLATFORM=win32 kui-build-electron",
    "build:electron:all": "kui-build-electron",
    "compile": "kui-compile",
    "watch": "tsc --build . --watch",
    "webpack": "if [ -z \"$TRAVIS_JOB_ID\" ]; then npm ci && npm run -s pty:nodejs; fi; CSP_ALLOWED_HOSTS=\"https://raw.githubusercontent.com http://localhost:8081 ws://localhost:8081 http://localhost:9953\" kui-watch-webpack",
    "proxy": "if [ -z \"$TRAVIS_JOB_ID\" ]; then npm run -s pty:nodejs; fi; kui-run-proxy",
    "pty:rebuild": "kui-pty-rebuild",
    "pty:electron": "npm run pty:rebuild electron",
    "pty:nodejs": "npm run pty:rebuild node",
    "test:electron": "KUI=$(echo \"$(pwd)/bin/oui-ascii\") kui-test",
    "test:webpack": "KUI_USE_PROXY=true MOCHA_RUN_TARGET=webpack WEBPACK_CLIENT_URL=http://localhost:9080 kui-test",
    "test": "npm run -s test:electron",
    "lint": "eslint '**/*.{js,ts}'",
    "format": "prettier --write '**/*.{css,html,js,json,md,ts}'",
    "postinstall": "npm run -s compile && npm run -s pty:electron",
    "start": "npm run -s postinstall && electron . shell"
  },
  "engines": {
    "node": ">=8.15.0",
    "npm": ">=5.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kui-shell/oui.git"
  },
  "keywords": [
    "openwhisk",
    "cli",
    "ui",
    "kui"
  ],
  "author": "@starpit",
  "contributors": [
    "Mengting Yan"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@kui-shell/builder": "5.5.8",
    "@kui-shell/core": "5.5.8",
    "@kui-shell/plugin-bash-like": "5.5.8",
    "@kui-shell/plugin-core-support": "5.5.8",
    "@kui-shell/plugin-editor": "5.5.8",
    "@kui-shell/plugin-proxy-support": "5.5.8",
    "@kui-shell/proxy": "5.5.8",
    "@kui-shell/test": "5.5.8",
    "@kui-shell/webpack": "5.5.8",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.14",
    "@types/swagger-schema-official": "^2.0.20",
    "@types/uuid": "3.4.6",
    "@types/webdriverio": "4.13.3",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "electron": "6.1.5",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "mocha": "6.2.2",
    "prettier": "1.19.1",
    "request-promise": "^4.2.5",
    "spectron": "8.0.0",
    "typescript": "3.7.3"
  },
  "dependencies": {
    "@kui-shell/plugin-apache-composer": "file:plugins/plugin-apache-composer",
    "@kui-shell/plugin-grid": "file:plugins/plugin-grid",
    "@kui-shell/plugin-openwhisk": "file:plugins/plugin-openwhisk",
    "@kui-shell/plugin-openwhisk-editor-extensions": "file:plugins/plugin-openwhisk-editor-extensions"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{css,html,js,json,md,ts}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  }
}
